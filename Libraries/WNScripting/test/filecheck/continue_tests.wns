// RUN: main 32 -> 2
// RUN: main 2 -> 1
// RUN: main 3 -> 2
Int main(Int x) {
  Int y = x;
  do {
    // CHECK: bool __wns_if_temp0 = false
    // CHECK-NEXT: {
    // CHECK-NEXT: __wns_if_temp0 = (y > 2);
    // CHECK-NEXT: }
    // CHECK-NEXT: if (__wns_if_temp0) {
    // CHECK-NEXT: {
    // CHECK-NEXT: continue;
    // CHECK-NEXT: }
    // CHECK-NEXT: }
    y = y - 1;
    if (y > 2) {
      continue;
    }
    return y;
  } while (true);
}
