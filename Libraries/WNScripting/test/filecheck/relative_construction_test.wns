struct Foo {
    Int x = 4;
    Int y = x + 4;
}

// CHECK: {{_Z3wns14_construct_FooENR3FooENUC3FooE}}
// CHECK-NEXT: _this->x = 4;
// CHECK-NEXT: _this->y = (_this->x + 4);
// CHECK-NEXT: return _this;
// RUN: main 32 -> 12
// RUN: main -32 -> 12
Int main(Int x) {
    Foo f = Foo();
    return f.x + f.y;
}
